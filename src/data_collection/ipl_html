<script>
    function homepageVideoJsTag(){
        $('.h_banner_vid_div').each(function(i,v){
            $id = $(v).data('id');
            $el_id = 'myAutoPlayVid_'+$id;
            $html = '<div><video-js id="'+$el_id+'" data-account="3588749423001" data-video-id="" data-player="default" data-embed="default" data-tracker-type="VIDEO" data-tracker-action="VIEW" data-application-id="" controls="true" autoplay="true" muted="muted" width="100" height="100"></video-js></div>';

            if($('#'+$el_id).length < 1){
                $(v).html($html);
            }
        });
        var script = document.createElement('script');
        script.src = '//players.brightcove.net/3588749423001/default_default/index.min.js';
        document.head.appendChild(script);
    }

    $(document).ready(function(){
        homepageVideoJsTag();
        // Flag to track if AJAX call has been made
        var ajaxCallMade = false;
        var isFetchData = true;
        function fetchHomeData() {
            if(isFetchData == false){
                return;
            }
            $('#sect-dy-data').css('display', 'flex');
            isFetchData = false;
            $.ajax({
                url: 'https://www.iplt20.com/gethomepageData',
                method: 'GET',
                success: function(response) {
                    $('#sect-dy-data').hide();
                    if(response.status == true){
                        $('.story-trey').html(response.story);
                        $(response.html).insertAfter('#sect-dy-data');

                        // $('#sect-dy-data').append(response.html);
                        ajaxCallMade = true;
                        initHomapgeSliders();
                        homepageVideoJsTag();
                    }
                },
                error: function(xhr, status, error) {
                    console.error('Error fetching data:', error);
                }
            });
        }
        $('body').on('scroll', function() {
            if(!ajaxCallMade) {
                fetchHomeData();
            }
        });
    });
</script>
 
